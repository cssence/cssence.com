  "repository": {
    "type": "git",
    "url": "https://github.com/cssence/cssence.com.git"
  },
  "scripts": {
    "build:clean": "node ssg/clean.mjs",
    "build:assets": "node ssg/copy.mjs",
    "build:json": "node ssg/render/json.mjs",
    "build:html": "node ssg/render/html.mjs",
